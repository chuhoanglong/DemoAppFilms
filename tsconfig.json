{
  "extends": "@react-native/typescript-config/tsconfig.json" /* Recommended React Native TSConfig base */,
  "compilerOptions": {
    "jsx": "react",
    /* Completeness */
    "skipLibCheck": true /* Skip type checking all .d.ts files. */,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "app/*": ["./app/*"]
    }
  },
  "typeRoots": ["./app/types"]
}
